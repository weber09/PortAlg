	(
    <ESCREVA> { line = token.beginLine; writeLine = false; } 
  | <ESCREVAL> { line = token.beginLine; writeLine = true; }
  ) 
  <LPAREN> 
  expression = Expression()
   {expressionList.add(expression); } 
  ( <COMMA> 
    expression = Expression()
    { expressionList.add(expression); } 
  )*
  <RPAREN>
  {
    return new SPWriteStatement(line, writeLine, expressionList);
  }
}

